{
    "name": "laravel-notification-channels/interfax",
    "description": "This package makes it easy to send PDFs as faxes via InterFAX with Laravel notifications",
    "homepage": "https://github.com/laravel-notification-channels/interfax",
    "license": "MIT",
    "authors": [
        {
            "name": "Craig Spivack",
            "email": "craig.spivack@ivinteractive.com",
            "homepage": "https://www.ivinteractive.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/notifications": "5.8.*|^6.0",
        "illuminate/support": "5.8.*|^6.0",
        "interfax/interfax": "^1.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "mpdf/mpdf": "^8.0",
        "orchestra/testbench": "~3.8|~4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Interfax\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Interfax\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
